{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/forbid-prop-types": ["error", {"forbid": ["any"]}],
    "react/no-unused-prop-types": ["error", {"skipShapeProps": true}]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "infra/webpack/config.js"
      }
    }
  }
}
